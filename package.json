{
  "name": "express-ecommerce-api",
  "version": "1.0.0",
  "description": "Complete e-commerce API built with Express.js - Migrated from Medusa.js",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc --project tsconfig.production.json",
    "build:production": "npm run clean && tsc --project tsconfig.production.json",
    "start": "node dist/server.js",
    "dev": "tsx watch express-src/server.ts",
    "migrate": "tsx express-src/database/migrate.ts",
    "migrate:generate": "drizzle-kit generate:pg",
    "migrate:reset": "drizzle-kit drop && npm run migrate:generate && npm run migrate",
    "seed": "tsx express-src/scripts/seed.ts",
    "db:setup": "npm run migrate:generate && npm run migrate && npm run seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
    "test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit",
    "lint": "eslint express-src --ext .ts,.js",
    "lint:fix": "eslint express-src --ext .ts,.js --fix",
    "format": "prettier --write express-src/**/*.{ts,js,json}",
    "deploy:render": "npm run build:production && npm start",
    "health": "curl -f http://localhost:9000/health || exit 1",
    "clean": "node -e \"const fs = require('fs'); const path = require('path'); const distPath = path.join(__dirname, 'dist'); if (fs.existsSync(distPath)) { fs.rmSync(distPath, { recursive: true, force: true }); console.log('Clean completed'); } else { console.log('No dist folder to clean'); }\""
  },
  "keywords": [
    "express",
    "ecommerce",
    "api",
    "typescript",
    "postgresql",
    "supabase",
    "rest",
    "nodejs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cookie-parser": "^1.4.6",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "pg": "^8.11.3",
    "drizzle-orm": "^0.29.1",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "slugify": "^1.6.6",
    "stripe": "^14.9.0",
    "nodemailer": "^6.9.7",
    "rate-limiter-flexible": "^4.0.1",
    "winston": "^3.11.0",
    "typescript": "^5.3.2",
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/multer": "^1.4.11",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@types/nodemailer": "^6.4.14",
    "@types/winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/supertest": "^6.0.2",
    "tsx": "^4.6.0",
    "drizzle-kit": "^0.20.6",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=20"
  }
}
